#include <iostream>

int main() {
    int edad;
    std::cout << "Ingresa tu edad: ";
    std::cin >> edad;

    // Usamos un operador de comparaciÃ³n (>=) dentro de una condiciÃ³n if
    if (edad >= 18) {
        std::cout << "Eres mayor de edad." << std::endl;
    } else {
        std::cout << "Eres menor de edad." << std::endl;
    }

    return 0;
}