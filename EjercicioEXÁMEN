#include <iostream>
using namespace std;
int main() {
    double a, b;
    cout << "Ingrese dos números: "; cin >> a >> b;
    if(a > b) {
        cout << "El mayor es: " << a << endl;
    } else if(b > a) {
        cout << "El mayor es: " << b << endl;
    } else {
        cout << "Los números son iguales" << endl;
    }
    return 0;
}






